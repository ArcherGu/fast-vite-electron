{
  "name": "fast-vite-electron",
  "version": "0.0.1",
  "packageManager": "pnpm@8.11.0",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vue-tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:main && npm run test:render",
    "test:render": "vitest run -r src/render --passWithNoTests",
    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "einf": "^1.5.0",
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "1.0.6",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/compiler-sfc": "3.3.11",
    "@vue/test-utils": "2.4.3",
    "electron": "28.0.0",
    "electron-builder": "24.9.1",
    "eslint": "8.55.0",
    "happy-dom": "12.10.3",
    "lint-staged": "15.2.0",
    "playwright": "1.40.1",
    "rimraf": "5.0.5",
    "simple-git-hooks": "2.9.0",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.6",
    "vite-plugin-doubleshot": "0.0.13",
    "vitest": "1.0.2",
    "vue-tsc": "1.8.25"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
