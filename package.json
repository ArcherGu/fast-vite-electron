{
  "name": "fast-vite-electron",
  "version": "0.0.1",
  "packageManager": "pnpm@10.28.0",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:main && npm run test:render",
    "test:render": "vitest run -r src/render --passWithNoTests",
    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "npx simple-git-hooks && electron-builder install-app-deps"
  },
  "dependencies": {
    "einf": "^1.5.3",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "1.0.141",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/compiler-sfc": "3.5.26",
    "@vue/test-utils": "2.4.6",
    "electron": "39.2.7",
    "electron-builder": "26.4.0",
    "eslint": "9.39.2",
    "happy-dom": "20.3.0",
    "lint-staged": "16.2.7",
    "playwright": "1.57.0",
    "rimraf": "6.1.2",
    "simple-git-hooks": "2.13.1",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-doubleshot": "0.0.20",
    "vitest": "4.0.17",
    "vue-tsc": "3.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "electron",
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
