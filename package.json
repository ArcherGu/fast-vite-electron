{
  "name": "fast-vite-electron",
  "version": "0.0.1",
  "packageManager": "pnpm@7.5.2",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf dist && vite",
    "build": "rimraf dist && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:main && npm run test:render",
    "test:render": "vitest run -r src/render --passWithNoTests",
    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "bytenode": "^1.3.6",
    "reflect-metadata": "^0.1.13",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "0.2.19",
    "@lightwing/eslint-config": "0.0.3",
    "@vitejs/plugin-vue": "3.0.1",
    "@vue/compiler-sfc": "3.2.37",
    "@vue/test-utils": "2.0.2",
    "chalk": "5.0.1",
    "electron": "19.0.8",
    "electron-builder": "23.1.0",
    "esbuild": "0.14.49",
    "eslint": "8.20.0",
    "happy-dom": "6.0.4",
    "kolorist": "1.5.1",
    "lint-staged": "13.0.3",
    "playwright": "1.23.4",
    "pnpm": "7.5.2",
    "rimraf": "3.0.2",
    "simple-git-hooks": "2.8.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.2",
    "vitest": "0.18.1",
    "vue-tsc": "0.38.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
