{
  "name": "fast-vite-electron",
  "version": "0.0.1",
  "packageManager": "pnpm@10.11.1",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf dist && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:main && npm run test:render",
    "test:render": "vitest run -r src/render --passWithNoTests",
    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "einf": "^1.5.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "1.0.117",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/compiler-sfc": "3.5.16",
    "@vue/test-utils": "2.4.6",
    "electron": "36.3.2",
    "electron-builder": "26.0.12",
    "eslint": "9.28.0",
    "happy-dom": "17.6.1",
    "lint-staged": "16.1.0",
    "playwright": "1.52.0",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.13.0",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-doubleshot": "0.0.18",
    "vitest": "3.2.1",
    "vue-tsc": "2.2.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "electron",
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
