{
  "name": "fast-vite-electron",
  "version": "0.0.1",
  "packageManager": "pnpm@8.9.2",
  "description": "Vite + Electron with esbuild, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf dist && vite",
    "build": "rimraf dist && vue-tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:main && npm run test:render",
    "test:render": "vitest run -r src/render --passWithNoTests",
    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "einf": "^1.5.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "0.0.24",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/compiler-sfc": "3.3.6",
    "@vue/test-utils": "2.4.1",
    "electron": "27.0.2",
    "electron-builder": "24.6.4",
    "eslint": "8.52.0",
    "happy-dom": "12.9.1",
    "lint-staged": "15.0.2",
    "playwright": "1.39.0",
    "rimraf": "5.0.5",
    "simple-git-hooks": "2.9.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-plugin-doubleshot": "0.0.12",
    "vitest": "0.34.6",
    "vue-tsc": "1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
